[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[user]
	name = JP Slavinsky
	email = jpslav@gmail.com
[color]
	ui = true
[core]
	excludesfile = ~/.gitignore
[pull]
	ff = only
